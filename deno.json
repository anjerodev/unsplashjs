{
  "name": "@anjerodev/unsplashjs",
  "version": "0.2.0",
  "exports": "./src/main.ts",
  "tasks": {
    "dev": "deno run --allow-net src/main.ts",
    "dev:watch": "deno run --allow-net --watch src/main.ts",
    "format": "deno fmt src/",
    "lint": "deno lint src/",
    "test": "deno test --allow-net src/"
  },
  "imports": {
    "@/": "./src/",
    "@c4spar/mock-fetch": "jsr:@c4spar/mock-fetch@^1.0.0",
    "@std/assert": "jsr:@std/assert@1",
    "@std/testing": "jsr:@std/testing@1"
  },
  "lint": {
    "rules": {
      "exclude": ["no-explicit-any"]
    }
  },
  "fmt": {
    "semiColons": false,
    "singleQuote": true
  }
}
